#include "qcom-msm8974.dtsi"

/ {
	model = "Qualcomm APQ8074 Dragonboard";
	compatible = "qcom,apq8074-dragonboard", "qcom,apq8074";

	soc: soc {
		sdhci@f9824900 {
			bus-width = <8>;
			non-removable;
			status = "ok";
		};

		sdhci@f98a4900 {
			cd-gpios = <&msmgpio 62 0x1>;
			bus-width = <4>;
		};
	};
};
