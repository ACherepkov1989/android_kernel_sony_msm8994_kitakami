				};
			};
		};

		sdc2_cd_on: sdc2_cd_on {
			mux {
				pins = "gpio38";
				function = "gpio";
			};

			config {
				pins = "gpio38";
				bias-pull-up;		/* pull up */
				drive-strength = <2>;	/* 2 MA */
