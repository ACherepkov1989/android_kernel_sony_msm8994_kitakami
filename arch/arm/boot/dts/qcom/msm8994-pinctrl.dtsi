		mhl_intr_active: mhl_intr_active {
			mux {
				pins = "gpio57";
				function = "gpio";
			};

			config {
				pins = "gpio57";
				bias-pull-up;
				input-enable;
			};
		};

		mhl_reset_active: mhl_reset_active {
			mux {
				pins = "gpio58";
				function = "gpio";
			};

			config {
				pins = "gpio58";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};

		spi0 {

			spi0_default: spi0_default {
				/* active state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio0", "gpio1", "gpio3";
					function = "blsp_spi1";
				};

				config {
					pins = "gpio0", "gpio1", "gpio3";
					drive-strength = <12>; /* 12 MA */
					bias-disable = <0>; /* No PULL */
				};
			};


			spi0_sleep: spi0_sleep {
				/* suspended state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio0", "gpio1", "gpio3";
					function = "gpio";
				};

				config {
					pins = "gpio0", "gpio1", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-pull-down; /* PULL Down */
				};
			};

			spi0_cs1_active: cs1_active {
				/* CS */
				mux {
					pins = "gpio8";
					function = "blsp_spi1_cs1";
				};

				config {
					pins = "gpio8";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};

			spi0_cs1_sleep: cs1_sleep {
				/* CS */
				mux {
					pins = "gpio8";
					function = "gpio";
				};

				config {
					pins = "gpio8";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};
		};

		pmx_i2c_1 {
			/* active state */
			i2c_1_active: i2c_1_active{
				mux {
					/* CLK, DATA */
					pins = "gpio2", "gpio3";
					function = "blsp_i2c1";
				};

				config {
					pins = "gpio2", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-disable; /* No PULL */
				};
			};

			/* suspended state */
			i2c_1_sleep: i2c_1_sleep {
				mux {
					/* CLK, DATA */
					pins = "gpio2", "gpio3";
					function = "blsp_i2c2";
				};

				config {
					pins = "gpio2", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-disable; /* No PULL */
				};
			};
		};

		audio_ext_devices {
			/* default state */
			audio_ext_speakers: audio_ext_speakers {
				mux {
