&mdss_dsi_active {
	mux {
		pins = "gpio25";
		function = "gpio";
	};
	config {
		pins = "gpio25";
		drive-strength = <2>; /* 2 mA */
		bias-disable = <0>; /* no pull */
		output-high;
	};
};

&mdss_dsi_suspend {
	mux {
		pins = "gpio25";
		function = "gpio";
	};
	config {
		pins = "gpio25";
		drive-strength = <2>; /* 2 mA */
		/delete-property/ bias-pull-down;
		bias-disable = <0>; /* no pull */
		output-low;
	};
};

&cam_sensor_mclk0_sleep {
	config {
		/delete-property/ bias-pull-down;
		bias-disable;
		output-low;
	};
};

